{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource",
                "{PROJECT_DIR}/resource_bilibili"
            ]
        }
    ],
    "task": [
        {
            "name": "登录游戏",
            "entry": "登录游戏",
            "pipeline_override": {
                "通用-确认获得物品": {
                    "next": [
                        "通用-返回",
                        "通用-返回主页"
                    ]
                },
                "通用-返回": {
                    "next": [
                        "登录游戏2"
                    ]
                },
                "通用-返回主页": {
                    "next": [
                        "登录游戏2"
                    ]
                }
            }
        },
        {
            "name": "基建",
            "entry": "收菜",
            "option": [
                "驻员",
                "矿场",
                "交易所",
                "浏览好友订单",
                "好友界面向左滑动"
            ]
        },
        {
            "name": "免费礼包+限时贸易所折扣物品",
            "entry": "补给站",
            "option": [
                "购买所有折扣物品",
                "购买-技能3",
                "购买-技能2",
                "购买-幻金能源"

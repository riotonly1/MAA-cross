        },
        {
            "name": "模拟军演",
            "entry": "模拟军演任务列表",
            "option": [
                "战力刷新选择"
            ]
        },
        {
            "name": "每日探索",
            "entry": "出击任务列表",
            "option": [
                "关卡选择",
                "第几层",
                "是否手动选择次数(开启燃料使用)",

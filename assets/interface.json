{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource",
                "{PROJECT_DIR}/resource_bilibili"
            ]
        }
    ],
    "task": [
        {
            "name": "登录游戏",
            "entry": "登录游戏"
        },
        {
            "name": "基建",
            "entry": "收菜"
        },
        {
            "name": "免费礼包+限时贸易所折扣物品",
            "entry": "补给站"
        },
        {
            "name": "模拟军演",
            "entry": "模拟军演任务列表"
        },
        {
            "name": "每日探索",
            "entry": "出击任务列表",
            "option": [
                "关卡选择"
              ] 
        },
        {
            "name": "日常领取",
            "entry": "领取任务列表"           
        },
        {
            "name": "通行证领取",
            "entry": "通行证任务列表"           
        },
        {
          "name": "活动",
          "entry": "活动任务列表",
          "option": [
                "活动选择"
              ]           
      },             
        {
            "name": "周本",
            "entry": "周本"
        }
    ],
    "option": {
        "关卡选择": {
          "cases": [
            {
              "name": "资源采集-技能磨砺",
              "pipeline_override": {
                "出击-选择": {
                "expected": "每日探索",
                "next":["每日探索-选择"]
                },
                "每日探索-选择": {
                "expected" : "资源采集",
                "next": ["每日探索-选择2"]
                },
                "每日探索-选择2": {
                  "expected" : "技能磨砺",
                  "next":[ "每日探索-第n层"]
                },
                "每日探索-第n层": {
                "expected" : "第4层"
                }
              }
            },
            {
                "name": "资源采集-星币开采",
                "pipeline_override": {
                  "出击-选择": {
                  "expected": "每日探索",
                  "next":["每日探索-选择"]
                  },
                  "每日探索-选择": {
                  "expected" : "资源采集",
                  "next": ["每日探索-选择2"]
                  },
                  "每日探索-选择2": {
                    "expected" : "星币开采",
                    "next":[ "每日探索-第n层"]
                  },
                  "每日探索-第n层": {
                  "expected" : "第6层"
                  }
                }
              },
              {
                  "name": "资源采集-技术解析",
                  "pipeline_override": {
                    "出击-选择": {
                    "expected": "每日探索",
                    "next":["每日探索-选择"]
                    },
                    "每日探索-选择": {
                    "expected" : "资源采集",
                    "next": ["每日探索-选择2"]
                    },
                    "每日探索-选择2": {
                      "expected" : "技术解析",
                      "next":[ "每日探索-第n层"]
                    },
                    "每日探索-第n层": {
                    "expected" : "第6层"
                    }
                  }
                },
                {
                    "name": "资源采集-荒墟拾遗",
                    "pipeline_override": {
                      "出击-选择": {
                      "expected" : "荒墟拾遗",
                      "next":["每日探索-选择"]
                      },
                      "每日探索-选择": {
                      "expected" : "资源采集",
                      "next": ["每日探索-选择2"]
                      },
                      "每日探索-选择2": {
                        "expected" : "荒墟拾遗",
                        "next":[ "每日探索-第n层"]
                      },
                      "每日探索-第n层": {
                      "expected" : "第4层"
                      }
                    }
                  },
                  {
                    "name": "跃升行动-自然跃升",
                    "pipeline_override": {
                      "出击-选择": {
                      "expected": "每日探索",
                      "next":["每日探索-选择"]
                      },
                      "每日探索-选择": {
                      "expected" : "跃升行动",
                      "next": ["每日探索-选择2"]
                      },
                      "每日探索-选择2": {
                        "expected" : "自然跃升",
                        "next":[ "每日探索-第n层"]
                      },
                      "每日探索-第n层": {
                      "expected" : "第2层"
                      }
                    }
                  },
                  {
                    "name": "跃升行动-人造跃升",
                    "pipeline_override": {
                      "出击-选择": {
                      "expected": "每日探索",
                      "next":["每日探索-选择"]
                      },
                      "每日探索-选择": {
                      "expected" : "跃升行动",
                      "next": ["每日探索-选择2"]
                      },
                      "每日探索-选择2": {
                        "expected" : "人造跃升",
                        "next":[ "每日探索-第n层"]
                      },
                      "每日探索-第n层": {
                      "expected" : "第2层"
                      }
                    }
                    },
                    {
                        "name": "跃升行动-生命跃升",
                        "pipeline_override": {
                          "出击-选择": {
                          "expected": "每日探索",
                          "next":["每日探索-选择"]
                          },
                          "每日探索-选择": {
                          "expected" : "跃升行动",
                          "next": ["每日探索-选择2"]
                          },
                          "每日探索-选择2": {
                            "expected" : "生命跃升",
                            "next":[ "每日探索-第n层"]
                          },
                          "每日探索-第n层": {
                          "expected" : "第2层"
                        }
                      }
                  }
              ]
          },
        "活动选择": {
          "cases": [
            {
              "name": "云端行迹",
              "pipeline_override": {
                "点击活动": {
                  "expected": "云端行迹"
                },
                "活动-领取门票": {
                "expected" : "旅记",
                "next": ["活动-领取"]
                },
                "活动-领取": {
                  "expected" : "领取",
                  "next":[ "通用-确认获得物品"]
                },
                "通用-确认获得物品": {
                "next":[ "通用-返回"]
                },
                "通用-返回": {
                "next":[ "活动-选择关卡"]
                },
                "活动-选择关卡": {
                "expected" : "画卷",
                "next": ["活动-选择模式"]
                },
                "活动-选择模式": {
                "expected" : "困难",
                "next": ["活动-滑动选择关卡"]
                },
                "活动-选择第几关": {
                "expected" : "超限阵列",
                "next": ["扫荡"]
              }
            }
        }
    ]
}
    },             
    "version": "v1.0.5"
}